<odoo>
    <record id="l10n_pe_edi_catalog_tmpl_view_tree" model="ir.ui.view">
        <field name="name">l10n_pe_edi.catalog.tmpl.tree</field>
        <field name="model">l10n_pe_edi.catalog.tmpl</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="code" />
                <field name="name" />
                <field name="complete_name" optional="hide" />
                <field name="active" optional="hide" widget="boolean_toggle" />
