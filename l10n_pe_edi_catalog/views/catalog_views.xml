        <field name="inherit_id" ref="l10n_pe_edi_catalog_tmpl_view_tree" />
        <field name="mode">primary</field>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute
                    name="string"
                >Catalog No. 03: Codigos de Tipo de unidades de medida comercial</attribute>
            </xpath>
        </field>
    </record>
    <record id="l10n_pe_edi_catalog_03_action" model="ir.actions.act_window">
